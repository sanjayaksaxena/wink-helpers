{
  "name": "wink-helpers",
  "version": "1.4.0",
  "description": "Low level helper functions for Javascript array, object and string",
  "keywords": [
    "Array",
    "Object",
    "String",
    "Helper",
    "Cartesian Product",
    "Frequency Table",
    "Validate",
    "Validation",
    "Sort",
    "Pluck",
    "Shuffle",
    "Random Shuffle"
  ],
  "main": "src/wink-helpers.js",
  "scripts": {
    "pretest": "npm run lint && npm run hint",
    "test": "nyc --reporter=html --reporter=text mocha ./test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "docco -o ./docs/ ./src/*.js",
    "hint": "jshint ./src/*.js ./test/*.js",
    "lint": "eslint ./src/*.js ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winkjs/wink-helpers.git"
  },
  "author": "Sanjaya Kumar Saxena",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/winkjs/wink-helpers/issues"
  },
  "homepage": "http://winkjs.org/",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "docco": "^0.8.0",
    "eslint": "^4.19.1",
    "nyc": "^14.1.1",
    "jshint": "^2.9.5",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "runkitExampleFilename": "runkit/example.js"
}
